
public abstract class Test {
	
	public abstract void enumMethod();
	
	enum TestingEnum {Test1,Test2, Test3; public void enumMethod() {System.out.println("EnumMethod");}}
	
	public static void main (String[]a){
		System.out.println("Run now ...");
		System.out.println("Enum : " + TestingEnum.Test1);
		System.out.println("Enum : " + Test.TestingEnum.Test2);
		System.out.println("Enum : " + TestingEnum.);
	}
}
